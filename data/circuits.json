{
    "circuits": {
        "individual": {
            "name": "Индивидуальный контур",
            "description": "Фокусируется на личной трансформации, уникальности, самореализации и привнесении мутаций в мир. Ключевое слово: Трансформация. Сенсорная ориентация: Акустическая.",
            "sub_circuits": {
                "knowing": {
                    "name": "Контур Знания",
                    "description": "Инсайты, внутренняя правда, оригинальность мышления и глубокая креативность."
                },
                "centering": {
                    "name": "Контур Центрирования",
                    "description": "Опора на себя, безусловная любовь к себе и следование своей уникальной природе."
                },
                "integration": {
                    "name": "Каналы Интеграции",
                    "description": "Базовая энергия выживания, самосохранения и адаптации в моменте ('здесь и сейчас')."
                }
            }
        },
        "collective": {
            "name": "Коллективный контур",
            "description": "Направлен на передачу опыта, обучение и развитие человечества. Ключевое слово: Изменение. Сенсорная ориентация: Визуальная.",
            "sub_circuits": {
                "logic": {
                    "name": "Контур Понимания (Логический)",
                    "description": "Фокусируется на выявлении паттернов, тестировании гипотез и создании логических решений для будущего."
                },
                "abstract": {
                    "name": "Контур Ощущения (Абстрактный)",
                    "description": "Направлен на осмысление прошлого опыта, рефлексию и циклическое развитие."
                }
            }
        },
        "tribal": {
            "name": "Племенной контур",
            "description": "Фокусируется на семье, поддержке, защите ресурсов и выживании сообщества. Ключевое слово: Традиция. Сенсорная ориентация: Тактильная и обонятельная.",
            "sub_circuits": {
                "ego": {
                    "name": "Контур Эго",
                    "description": "Материальное обеспечение племени, договоры, бизнес и взаимная поддержка."
                },
                "defense": {
                    "name": "Контур Защиты",
                    "description": "Забота о близких, воспитание детей и поддержание стабильности сообщества."
                }
            }
        }
    },
